        "emailVerified": {
            "$id": "#/properties/emailVerified",
            "type": "boolean",
            "title": "The emailVerified schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                true
            ]
        },
        "createDate": {
            "$id": "#/properties/createDate",
            "type": "string",
            "title": "The createDate schema",
            "description": "An explanation about the purpose of this instance.",
